    <script src="_framework/blazor.server.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?v=3.39&libraries=places&key=AIzaSyAIztt-WDuygbYykfzV8akJ3DyR-jrhJRY&callback=initializeMap"></script>
    <script async defer src="js/interop.js"></script>
